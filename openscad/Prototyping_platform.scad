module Unit(   Unit_box_size_X,Unit_box_size_Y,Unit_box_size_Z
                        ,Unit_position_X,Unit_position_Y,Unit_position_Z,Unit_rotation
                        ,Unit_post_1_X,Unit_post_1_Y
                        ,Unit_post_2_X,Unit_post_2_Y
                        ,Unit_post_3_X,Unit_post_3_Y
                        ,Unit_post_4_X,Unit_post_4_Y
                        ,Unit_post_base_diameter,Unit_post_base_heigth
                        ,Unit_post_diameter,Unit_post_total_heigth
                        ,Unit_post_hole_diameter
                        ,Unit_color,Unit_color_trasparency,Unit_show_box
                        ,Unit_stl,Unit_stl_position_X,Unit_stl_position_Y,Unit_stl_position_Z
                        ,Unit_description)
   {
   translate([0,0,0])
      {
      difference()
         {
         union()
            {
            if(Unit_show_box==true)
               color(Unit_color,Unit_color_trasparency)
               translate([0,0,Unit_post_total_heigth+Unit_position_Z+Unit_box_size_Z/2])   
               %cube([Unit_box_size_X,Unit_box_size_Y,Unit_box_size_Z],center=true);

            %color("Black")
               translate([0,0,Unit_post_total_heigth+Unit_position_Z+Unit_box_size_Z+5])
                  linear_extrude(1)
                     text(Unit_description,size=5,halign="center",valign="center");

            if(Unit_stl!="")
            color(Unit_color,Unit_color_trasparency)
            translate([0,0,Unit_post_total_heigth+Unit_position_Z])
            translate([Unit_stl_position_X,Unit_stl_position_Y,Unit_stl_position_Z])
            %import(Unit_stl);
            

               color(Unit_color)
               {
               translate([Unit_post_1_X,Unit_post_1_Y,0])
                  Post(Unit_post_base_diameter,Unit_post_base_heigth,Unit_post_diameter,Unit_post_total_heigth,Unit_post_hole_diameter,"1");

               translate([Unit_post_2_X,Unit_post_2_Y,0])
                  Post(Unit_post_base_diameter,Unit_post_base_heigth,Unit_post_diameter,Unit_post_total_heigth,Unit_post_hole_diameter,"2");

               translate([Unit_post_3_X,Unit_post_3_Y,0])
                  Post(Unit_post_base_diameter,Unit_post_base_heigth,Unit_post_diameter,Unit_post_total_heigth,Unit_post_hole_diameter,"3");

               translate([Unit_post_4_X,Unit_post_4_Y,0])
                  Post(Unit_post_base_diameter,Unit_post_base_heigth,Unit_post_diameter,Unit_post_total_heigth,Unit_post_hole_diameter,"4");

               hull()
                  {
                     translate([Unit_post_1_X,Unit_post_1_Y,0])
                        cylinder(d=Unit_post_base_diameter,h=Unit_post_base_heigth);

                     translate([Unit_post_2_X,Unit_post_2_Y,0])
                        cylinder(d=Unit_post_base_diameter,h=Unit_post_base_heigth);         
                  }

               hull()
                  {
                     translate([Unit_post_2_X,Unit_post_2_Y,0])
                        cylinder(d=Unit_post_base_diameter,h=Unit_post_base_heigth);

                     translate([Unit_post_3_X,Unit_post_3_Y,0])
                        cylinder(d=Unit_post_base_diameter,h=Unit_post_base_heigth);
                  }

               hull()
                  {
                     translate([Unit_post_3_X,Unit_post_3_Y,0])
                        cylinder(d=Unit_post_base_diameter,h=Unit_post_base_heigth);

                     translate([Unit_post_4_X,Unit_post_4_Y,0])
                        cylinder(d=Unit_post_base_diameter,h=Unit_post_base_heigth);
                  }

               hull()
                  {
                     translate([Unit_post_4_X,Unit_post_4_Y,0])
                        cylinder(d=Unit_post_base_diameter,h=Unit_post_base_heigth);

                     translate([Unit_post_1_X,Unit_post_1_Y,0])
                        cylinder(d=Unit_post_base_diameter,h=Unit_post_base_heigth);
                  }

               if(Structure_middle_cross==true)
                  {
                  hull()
                     {
                        translate([Unit_post_2_X,Unit_post_2_Y,0])
                           cylinder(d=Unit_post_base_diameter,h=Unit_post_base_heigth);

                        translate([Unit_post_4_X,Unit_post_4_Y,0])
                           cylinder(d=Unit_post_base_diameter,h=Unit_post_base_heigth);
                     }

                  hull()
                     {
                        translate([Unit_post_1_X,Unit_post_1_Y,0])
                           cylinder(d=Unit_post_base_diameter,h=Unit_post_base_heigth);

                        translate([Unit_post_3_X,Unit_post_3_Y,0])
                           cylinder(d=Unit_post_base_diameter,h=Unit_post_base_heigth);
                     }
                  }
               }
            }
            translate([Unit_post_1_X,Unit_post_1_Y,-1])
               cylinder(d=Unit_post_hole_diameter,h=Unit_box_size_Z+2);

            translate([Unit_post_2_X,Unit_post_2_Y,-1])
               cylinder(d=Unit_post_hole_diameter,h=Unit_box_size_Z+2);

            translate([Unit_post_3_X,Unit_post_3_Y,-1])
               cylinder(d=Unit_post_hole_diameter,h=Unit_box_size_Z+2);

            translate([Unit_post_4_X,Unit_post_4_Y,-1])
               cylinder(d=Unit_post_hole_diameter,h=Unit_box_size_Z+2);

            translate([0,0,Unit_post_total_heigth+Unit_position_Z+Unit_box_size_Z/2])   
               cube([Unit_box_size_X,Unit_box_size_Y,Unit_box_size_Z],center=true);
         }
      }
   }

module Post(A,B,C,D,E,F,G,H)
   {
   difference()
      {
      union()
         {
         cylinder(d=A,h=B);
         cylinder(d=C,h=D);

         %translate([5,5,0])
         linear_extrude(1)
            text(str(F),size=2);
         }
      translate([0,0,-1])
         cylinder(d=E,h=D+2);

      }
   
   }


