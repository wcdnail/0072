module Structures()
   color("Black")
   {
   hull()
      {
      translate([Structure_start_X,Structure_start_Y,0])
         cylinder(d=Structure_width,h=Structure_thickness);
      
      translate([Structure_start_X,Structure_end_Y,0])
         cylinder(d=Structure_width,h=Structure_thickness);
      }

   hull()
      {
      translate([Structure_start_X,Structure_start_Y,0])
         cylinder(d=Structure_width,h=Structure_thickness);
      
      translate([Structure_end_X,Structure_start_Y,0])
         cylinder(d=Structure_width,h=Structure_thickness);
      }

   hull()
      {
      translate([Structure_start_X,Structure_end_Y,0])
         cylinder(d=Structure_width,h=Structure_thickness);
      
      translate([Structure_end_X,Structure_end_Y,0])
         cylinder(d=Structure_width,h=Structure_thickness);
      }

   hull()
      {
      translate([Structure_end_X,Structure_start_Y,0])
         cylinder(d=Structure_width,h=Structure_thickness);
      
      translate([Structure_end_X,Structure_end_Y,0])
         cylinder(d=Structure_width,h=Structure_thickness);
      }

   StepY=(Structure_start_Y-Structure_end_Y)/Structure_segments_Y;
   translate([0,Structure_end_Y,0])
   for(a=[0:StepY:Structure_start_Y-Structure_end_Y])
      {
      color("Black")
      hull()
         {
         translate([Structure_start_X,a,0])
            cylinder(d=Structure_width,h=Structure_thickness);
         
         translate([Structure_end_X,a,0])
            cylinder(d=Structure_width,h=Structure_thickness);
         }
      }

   StepX=(Structure_start_X-Structure_end_X)/Structure_segments_X;
   
   translate([Structure_end_X,0,0])
   for(a=[0:StepX:Structure_start_X-Structure_end_X])
      {
      color("Black")
      hull()
         {
         translate([a,Structure_start_Y,0])
            cylinder(d=Structure_width,h=Structure_thickness);
         
         translate([a,Structure_end_Y,0])
            cylinder(d=Structure_width,h=Structure_thickness);
         }
      }
   }

